{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "WEX Inc., AWS Lambda tools Role.",
  "Resources": {
    "WexCloudFormationLambdaUtilitiesRole": {
      "Type": "AWS::IAM::Role",
      "Properties": {
        "RoleName": "AWS::NoValue",
        "AssumeRolePolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Principal": {
                "Service": [
                  "lambda.amazonaws.com"
                ]
              },
              "Action": [
                "sts:AssumeRole"
              ]
            }
          ]
        },
        "Path": "/",
        "Policies": [
          {
            "PolicyName": "root",
            "PolicyDocument": {
              "Version": "2012-10-17",
              "Statement": [
                {
                  "Effect": "Allow",
                  "Action": "*",
                  "Resource": "*"
                }
              ]
            }
          }
        ],
        "Tags": []
      }
    }
  },
  "Outputs": {
    "WexCloudFormationLambdaUtilitiesExport": {
      "Description": "IAM Role for CloudFormation Lambda Utils",
      "Value": {
        "Fn::GetAtt": [
          "WexCloudFormationLambdaUtilitiesRole",
          "Arn"
        ]
      },
      "Export": {
        "Name": "WexCloudFormationLambdaUtilitiesRole:Arn"
      }
    }
  }
}
