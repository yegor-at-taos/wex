{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "WEX Inc., AWS Lambda tools for CloudFormation.",
  "Resources": {
    "VpcTransformExecutionRole": {
      "Type": "AWS::IAM::Role",
      "Properties": {
        "AssumeRolePolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Principal": {
                "Service": [
                  "lambda.amazonaws.com"
                ]
              },
              "Action": [
                "sts:AssumeRole"
              ]
            }
          ]
        },
        "Path": "/",
        "Policies": [
          {
            "PolicyName": "root",
            "PolicyDocument": {
              "Version": "2012-10-17",
              "Statement": [
                {
                  "Effect": "Allow",
                  "Action": "*",
                  "Resource": "*"
                }
              ]
            }
          }
        ]
      }
    },
    "VpcTransformFunctionPermissions": {
      "Type": "AWS::Lambda::Permission",
      "Properties": {
        "Action": "lambda:InvokeFunction",
        "FunctionName": {
          "Fn::GetAtt": [
            "VpcTransformFunction",
            "Arn"
          ]
        },
        "Principal": "cloudformation.amazonaws.com"
      }
    },
    "VpcAutoAcceptFunctionPermissions": {
      "Type": "AWS::Lambda::Permission",
      "Properties": {
        "Action": "lambda:InvokeFunction",
        "FunctionName": {
          "Fn::GetAtt": [
            "VpcAutoAcceptFunction",
            "Arn"
          ]
        },
        "Principal": "cloudformation.amazonaws.com"
      }
    },
    "VpcAutoAcceptFunction": {
      "Type": "AWS::Lambda::Function",
      "Properties": {
        "Code": {
          "S3Bucket": "%%% TO BE UPDATED %%%",
          "S3Key": "VpcAutoAcceptFunction.zip"
        },
        "FunctionName": "VpcAutoAcceptFunction",
        "Handler": "VpcAutoAcceptFunction.handler",
        "Runtime": "python3.7",
        "Timeout": "5",
        "Role": {
          "Fn::GetAtt": [
            "VpcTransformExecutionRole",
            "Arn"
          ]
        },
        "Tags": [
          {
            "Key": "LastUpdatedTime",
            "Value": "AWS::NoValue"
          }
        ]
      }
    },
    "VpcTransformFunction": {
      "Type": "AWS::Lambda::Function",
      "Properties": {
        "Code": {
          "S3Bucket": "%%% TO BE UPDATED %%%",
          "S3Key": "VpcTransformFunction.zip"
        },
        "FunctionName": "VpcTransformFunction",
        "Handler": "VpcTransformFunction.handler",
        "Runtime": "python3.7",
        "Timeout": "5",
        "Role": {
          "Fn::GetAtt": [
            "VpcTransformExecutionRole",
            "Arn"
          ]
        },
        "Tags": [
          {
            "Key": "LastUpdatedTime",
            "Value": "AWS::NoValue"
          }
        ]
      }
    },
    "Transform": {
      "Type": "AWS::CloudFormation::Macro",
      "Properties": {
        "Name": "wexRouteFiftyThreeMacro",
        "FunctionName": {
          "Fn::GetAtt": [
            "VpcTransformFunction",
            "Arn"
          ]
        }
      }
    }
  },
  "Outputs": {
      "R53AutoAcceptLambda": {
          "Description": "Cross-account share auto-accept",
          "Value": {
              "Fn::GetAtt": [
                  "VpcAutoAcceptFunction",
                  "Arn"
              ]
          },
          "Export": {
              "Name": "VpcAutoAcceptFunction-Arn"
          }
      }
  }
}
